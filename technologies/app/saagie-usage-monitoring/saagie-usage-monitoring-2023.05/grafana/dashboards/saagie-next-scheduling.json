          "rawSql": "SELECT\n  instance_start_time AS \"time\",\n  project_name || ' - '|| orchestration_name  as metric,\n  instance_status as \" \"\nFROM supervision_saagie\nWHERE\n  $__timeFilter(instance_end_time)\n  and orchestration_type='job'\n  and project_name in (${Project})\nUNION ALL\nSELECT\n  instance_end_time AS \"time\",\n  project_name || ' - '|| orchestration_name  as metric,\n  null as \" \"\nFROM supervision_saagie\nWHERE\n  $__timeFilter(instance_end_time)\n  and orchestration_type='job'\n  and project_name in (${Project})\nUNION ALL\nSELECT \n  next_instance_start AS \"time\",\n  project_name || ' - '|| orchestration_name  as metric,\n  'NEXT_SCHEDULE' as \" \"\nFROM supervision_saagie_predict\nWHERE \n  $__timeFilter(next_instance_start)\n  and orchestration_type='job'\n  and project_name in (${Project})\nUNION ALL \nSELECT \n  next_instance_end AS \"time\",\n  project_name || ' - '|| orchestration_name  as metric,\n  null as \" \"\nFROM supervision_saagie_predict\nWHERE\n  $__timeFilter(next_instance_end)\n  and orchestration_type='job'\n  and project_name in (${Project})",
